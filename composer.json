{
  "name": "bbs-lab/nova-translation",
  "keywords": [
    "laravel",
    "nova",
    "translation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "SÃ©bastien Muller",
      "email": "smuller@big-boss-studio.com",
      "role": "Developer"
    },
    {
      "name": "BBS Lab",
      "email": "paris@big-boss-studio.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "bbs-lab/nova-cloudinary-field": "dev-master",
    "illuminate/contracts": "^6.2 || ^7.0",
    "illuminate/database": "^6.2 || ^7.0",
    "illuminate/support": "^6.2 || ^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "BBSLab\\NovaTranslation\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "BBSLab\\NovaTranslation\\NovaTranslationServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
